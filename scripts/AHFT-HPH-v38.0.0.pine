f_calculate_cvar_constrained_kelly(w, p, cvar_hist, tau) =>
    f = w - (1 - w) / p
    if cvar_hist > tau and f > 0
        lo_b = 0.0
        hi_b = f
        mid = f
        for i = 0 to 5
            mid := (lo_b + hi_b) / 2
            cvar_approx = mid * cvar_hist / f
            if cvar_approx > tau
                hi_b := mid
            else
                lo_b := mid
        f := lo_b
    math.max(0, math.min(f, 1))

